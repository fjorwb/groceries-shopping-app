{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "eslint:recommended", "plugin:react/recommended", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "space-before-function-paren": "off",
    "camelcase": ["error", { "properties": "never", "ignoreDestructuring": true }]
  }
}
